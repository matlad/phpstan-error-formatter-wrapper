{
  "name": "matla/phpstan-error-formatter-wrapper",
  "type": "phpstan-extension",
  "description": "allow wrap several error formatters",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "matla",
      "email": "email@example.com"
    }
  ],
  "require": {
    "phpstan/phpstan": ">=0.12.0"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "matla\\phpstanEfw\\": "src"
    }
  },
  "extra": {
    "phpstan": {
      "includes": [
        "extension.neon"
      ]
    }
  }
}